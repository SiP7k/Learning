using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Start
{
    class Program
    {
        public static void Main()
        {
            string name = "Chalcev";
            string surname = "Savva";
            Console.WriteLine(name + " " + surname);
            string tempName = name;
            name = surname;
            surname = tempName;
            Console.WriteLine(name + " " + surname);
        }
    }
}
