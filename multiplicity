using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Start
{
    class Program
    {
        public static void Main()
        {
            Random random = new Random();
            int maxNumber = 27;
            int minNumber = 1;
            int number = random.Next(minNumber, maxNumber + 1);
            int minResult = 100;
            int maxResult = 999;

            for (int result = number; result <= maxResult; result += number)
            {
                if (result >= minResult)
                {
                    Console.WriteLine(result);
                }
            }
        }
    }
}
